                <!-- Modal Agregar tratamiento -->
                {{#if esOdontologo}}
                <button type="button" class="btn style-button" data-bs-toggle="modal" data-bs-target="#exampleModal">
                    Nuevo Tratamiento
                </button>
                <div class="modal fade " id="exampleModal" tabindex="-1" aria-labelledby="Title" aria-hidden="true">
                    <div class="modal-dialog">
                        <div class="modal-content modal-styles">
                            <div class="modal-header">
                                <h5 class="modal-title fw-light" id="Title">Agregar Tratamiento</h5>
                                <button type="button" class="btn-close" data-bs-dismiss="modal" aria-label="Close"></button>
                            </div>
                            <form action="/treatments/new-treatment" method="POST">
                                <div class="modal-body">
                                    <div class="form-group">
                                        <!--<input type="number" name="codigo" class="form-control" placeholder="Codigo..."
                                    autofocus>-->
                                    </div>
                                    <div class="d-flex justify-content-center">
                                        <img src="/img/boca-dientes.png" height="200px">
                                    </div>
                                    <div class="form-group mt-4">
                                        <label for="name" class="form-label fw-bolder">Tratamientos</label>
                                        <select class="form-select" id="name" name="nombre">

                                            {{#each datosJson}}
                                            <option>{{Nombre}}</option>
                                            {{/each}}

                                        </select>
                                    </div>
                                    <div class="form-group mt-4">
                                        <label class="form-label fw-bold" for="fecha">Seleccione una fecha:</label>
                                        <input type="date" id="datepiker" class="form-control" name="fecha">
                                    </div>

                                    <div class="form-group mt-4" style="display: none;">
                                        <p><input type="text" name="nroDiente" value="{{nroDiente}}">{{nroDiente}}</p>
                                    </div>
                                    <div class="form-group mt-4">
                                        <label for="caraDiente" class="form-label fw-bold">Cara</label>
                                        <select class="form-select" id="caraDiente" name="caraDiente">

                                            <option>Distal</option>

                                            <option>Lingual</option>

                                            <option>Mesial</option>

                                            <option>Palatina</option>

                                            <option>Vestibular</option>

                                        </select>
                                    </div>

                                    <div class="form-group mt-4">
                                        <label for="paciente" class="form-label fw-bold"> Paciente</label>
                                        <select class="form-select" id="paciente" name="paciente">
                                            {{#each pacientes}}
                                            <option>{{dni}}, {{nombre}} {{apellido}}</option>
                                            {{/each}}
                                        </select>
                                    </div>

                                    <div class="form-floating">
                                        <textarea name="observaciones" class="form-control" placeholder="observaciones..."
                                            style="margin-top: 3%;" id="observaciones"></textarea>
                                        <label for="observaciones">observaciones...</label>
                                    </div>
                                </div>
                                <div class="modal-footer">
                                    <button class="btn btn-primary modal-button-save" type="submit">Guardar</button>
                                    <button type="button" class="btn btn-danger modal-button-delete"
                                        data-bs-dismiss="modal">Cancelar</button>
                                </div>
                            </form>
                        </div>
                    </div>
                </div>
                {{/if}}
            </div>
        </div>

